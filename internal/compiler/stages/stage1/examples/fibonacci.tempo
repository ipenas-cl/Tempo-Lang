// fibonacci.tempo - Ejemplo para probar stage1

function fibonacci(n: i32) -> i32 {
    if (n <= 1) {
        return n;
    }
    
    let a = fibonacci(n - 1);
    let b = fibonacci(n - 2);
    return a + b;
}

function main() {
    print("Fibonacci sequence:");
    
    let i = 0;
    while (i < 10) {
        let result = fibonacci(i);
        print("fib(");
        print_number(i);
        print(") = ");
        print_number(result);
        i = i + 1;
    }
}